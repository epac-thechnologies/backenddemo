server.port=8022
spring.application.name=Archive-Service
spring.cloud.dicovery.enabled=true
eureka.client.service-url.defaultZone=http://localhost:8761/EurekaService/eureka



# ===============================
# DATABASE
# ===============================

spring.datasource.driver-class-name=com.mysql.jdbc.Driver

spring.datasource.url=jdbc:mysql://localhost:3306/Archive?useUnicode=true&useJDBCCompliantTimezoneShift=true&useLegacyDatetimeCode=false&serverTimezone=UTC
spring.datasource.username=root
spring.datasource.password=root

# ===============================
# JPA / HIBERNATE
# ===============================

spring.jpa.show-sql=true
spring.jpa.hibernate.ddl-auto=update
spring.jpa.properties.hibernate.dialect=org.hibernate.dialect.MySQL5Dialect
spring.jpa.properties.hibernate.current_session_context_class=org.springframework.orm.hibernate5.SpringSessionContext

logging.file=/Users/stage/Desktop/spring-boot-app.log
logging.level.org.hibernate=error
logging.level.root=WARN
logging.level.org.springframework.web=DEBUG
